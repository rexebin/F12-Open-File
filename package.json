{
  "name": "f12-open-file",
  "displayName": "F12: Open File",
  "description": "Extending Go to definition to open file when the look up symbol is a file name with relative path.",
  "version": "0.3.2",
  "publisher": "rexebin",
  "repository": {
    "url": "https://github.com/rexebin/F12-Open-File"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "icon": "f12.png",
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "definition",
    "angular",
    "typescript",
    "javascript",
    "file"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.7.4",
    "vscode": "^1.1.36",
    "tslint": "^5.20.1",
    "@types/node": "^13.1.2",
    "@types/mocha": "^5.2.7"
  }
}
